{
  "name": "web-server",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=test ts-node-dev --transpile-only --ignore-watch node_modules src/index.ts",
    "typeorm": "ts-node-dev node_modules/typeorm/cli.js",
    "migrations:run": "yarn typeorm migration:run",
    "migrations:revert": "yarn typeorm migration:revert",
    "test": "NODE_ENV=test jest",
    "build": "tsc --build",
    "build:clean": "tsc --build --clean",
    "prepublish": "tsc --build"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "jest": "^27.3.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "express": "^4.17.1",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2"
  }
}
